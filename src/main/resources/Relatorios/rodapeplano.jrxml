	<queryString>
		<![CDATA[SELECT

	LINHA,
	TIPO_LINHA,
	RODPLAN_LINHA,
	CAST(RODPLAN_DATA AS datetime)DATE,
	RODPLAN_REV,
	RODPLAN_DESCRICAO,
	RODPLAN_EMITENTE,
	RODPLAN_SETOR,
	RODPLAN_RESPONSAVEL,
	RODPLAN_MATERIAL,
	SETOR,N_PLANO,
	CONTATO_CHAVE,
	EQUIPE_PRINCIPAL,
	CAST(DATA_INICIAL AS datetime) DATA
FROM

     Cad_RodPlan INNER JOIN Cad_Linha ON RODPLAN_LINHA = LINHA

where

RODPLAN_LINHA = $P{linha}
order by
RODPLAN_REV ASC]]>

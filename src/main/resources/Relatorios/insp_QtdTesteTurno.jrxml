	<queryString>
		<![CDATA[SELECT 	TURNO,
		COUNT(TURNO) QTD_TESTES
FROM (
	SELECT		a.ID_MATERIAL_ID,
				CONVERT(time, b.DATA_REGISTRO, 101) AS HORA_REGISTRO,
		    	CASE 	WHEN  CONVERT(time, b.DATA_REGISTRO, 101) BETWEEN '06:00:00' AND '15:00:00' THEN 'B'
		        		WHEN  CONVERT(time, b.DATA_REGISTRO, 101) BETWEEN '15:01:00' AND '23:30:00' THEN 'C'
		        		WHEN  CONVERT(time, b.DATA_REGISTRO, 101) BETWEEN '23:31:00' AND '23:59:00' THEN 'A'
		        		WHEN  CONVERT(time, b.DATA_REGISTRO, 101) BETWEEN '00:00:00' AND '05:59:00' THEN 'A'
		    	END AS TURNO
	FROM		Cad_EspecLab a
	INNER JOIN	Cad_ResultadoInsp b ON a.ID 		= b.ID_ESPECIFICACAO_ID
	WHERE		CONVERT(date, b.DATA_REGISTRO, 101) BETWEEN $P{dataini} AND $P{datafim}
) AS TB_TESTES
GROUP BY TURNO
ORDER BY TURNO]]>
	</queryString>

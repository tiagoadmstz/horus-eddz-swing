	<queryString>
		<![CDATA[select row_number() over(order by data_hora_input ASC) AS INDICE, * from (SELECT
DDZ.ID,
data_hora_input,
DATA_INPUT,
HORA_INPUT,
MATERIAL,
QTD_SCRAP,
QTD_SCRAP_REAIS,
ACAO_TOMADA,
DESC_PROBLEMA,
DESC_CAUSA,
DDZ.OBSERVACAO,
QTD_SCRAP_KG,
MAT_LINHA,
DESC_MATERIAL,
BPCS,
PESO,
EVIDENCIA,
(SELECT MAX(VELOCIDADE) FROM Velocidade_Perfil FULL OUTER JOIN Qry_Material ON ID_MATERIAL_ID = ID_MATERIAL WHERE MATERIAL = $P{perfil}) VELOCIDADE
FROM Problemas
INNER JOIN Causas ON Problemas.ID = Causas.ID_PROBLEMA_ID
INNER JOIN vw_ddz_datahora DDZ ON Causas.ID = DDZ.ID_CAUSA_ID
INNER JOIN Cad_Material ON DDZ.ID_M_MATERIAL = MATERIAL
WHERE
MATERIAL = $P{perfil}
 AND ddz.id_controle = dbo.ObterDdzIdControle($P{perfil}, $P{datetimeinput})
) as tb order by data_hora_input ASC]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT 	*,
		CAST(((QTD_REPROVADOS / (QTD_REPROVADOS + QTD_APROVADOS)) * 100) AS DECIMAL(10,3)) PRT_REPROVADOS
FROM (
	SELECT		a.ID_MATERIAL_ID,
				d.DESC_MATERIAL,
				CAST(SUM((CASE b.STATUS WHEN 'Reprovado' THEN 1 ELSE 0 END)) AS DECIMAL(10,3)) QTD_REPROVADOS,
				CAST(SUM((CASE b.STATUS WHEN 'Aprovado' THEN 1 ELSE 0 END)) AS DECIMAL(10,3)) QTD_APROVADOS
	FROM		Cad_EspecLab a
	INNER JOIN	Cad_ResultadoInsp b ON a.ID 		= b.ID_ESPECIFICACAO_ID
	INNER JOIN	Cad_Teste c 		ON c.COD_TESTE	= a.ID_TESTE_ID
	INNER JOIN	Cad_Material d 		ON d.MATERIAL 	= a.ID_MATERIAL_ID
	WHERE		CONVERT(date, b.DATA_REGISTRO, 101) BETWEEN $P{dataini} AND $P{datafim}
	GROUP BY	a.ID_MATERIAL_ID,
				d.DESC_MATERIAL
) AS TB_REPROVADOS]]>
	</queryString>

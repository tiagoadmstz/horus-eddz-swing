	<queryString>
		<![CDATA[SELECT
      CAST (ROD_DATA as datetime)data,
     Cad_Rodape."ROD_REV" AS ROD_REV,
     Cad_Rodape."ROD_DESCRICAO" AS ROD_DESCRICAO,
     Cad_Rodape."ROD_EMITENTE" AS ROD_EMITENTE,
     Cad_Rodape."ROD_SETOR" AS ROD_SETOR,
     Cad_Rodape."ROD_RESPONSAVEL" AS ROD_RESPONSAVEL,
     Cad_Rodape."ROD_MATERIAL" AS ROD_MATERIAL
FROM
     "dbo"."Cad_Rodape" Cad_Rodape

where
Cad_Rodape."ROD_MATERIAL" = $P{perfil}
and
(ROD_REV=(SELECT MIN(ROD_REV)FROM Cad_Rodape where ROD_MATERIAL =$P{perfil}) OR
ROD_REV=(SELECT MAX(ROD_REV) FROM Cad_Rodape where ROD_MATERIAL =$P{perfil}))]]>
	</queryString>
